uter.push({\r\n                    name:'Login'\r\n                });\r\n            }catch(error){\r\n                console.log(error);\r\n            }\r\n        }\r\n            const login = () => {\r\n            router.push({\r\n                name:'Login'\r\n            });\r\n            }\r\n            const join = () => {\r\n            router.push({\r\n                name:'Join'\r\n            });\r\n            }\r\n            const main = () => {\r\n            router.push({\r\n                name:'Main'\r\n            });\r\n            }\r\n            const About = () => {\r\n            router.push({\r\n                name:'About'\r\n            });\r\n            }\r\n            const Board_List = () => {\r\n            router.push({\r\n                name:'Board_List'\r\n            });\r\n            }\r\n            const Contact = () => {\r\n            router.push({\r\n                name:'Contact'\r\n            });\r\n            }\r\n        return{\r\n           logout, login, join, main, About, Board_List, Contact\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]},"metadata":{},"sourceType":"module"}