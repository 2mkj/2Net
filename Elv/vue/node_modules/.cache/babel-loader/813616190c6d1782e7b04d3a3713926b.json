{"ast":null,"code":"import axios from '../axios/axiossetting.js';\nimport { useRouter } from 'vue-router';\nexport default {\n  props: {\n    parent_id: {\n      type: String,\n      required: true\n    }\n  },\n\n  setup() {\n    //페이지 이동하기 위해 useRouter()를 사용합니다\n    const router = useRouter();\n\n    const logout = async () => {\n      try {\n        const res = await axios.post('member/logout');\n        console.log(\"여기는 logout:\" + res.data); //route의 이름이 'Login'인 경로로 이동합니다\n\n        router.push({\n          name: 'Login'\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }; //logout\n\n\n    return {\n      logout\n    };\n  } //setup\n\n\n};","map":{"version":3,"mappings":"AA6BE,OAAOA,KAAP,MAAkB,0BAAlB;AACA,SAAQC,SAAR,QAAwB,YAAxB;AACF,eAAe;EACbC,KAAK,EAAC;IACAC,SAAS,EAAC;MACRC,IAAI,EAACC,MADG;MAERC,QAAQ,EAAC;IAFD;EADV,CADO;;EAObC,KAAK,GAAE;IACL;IACA,MAAMC,MAAK,GAAIP,SAAS,EAAxB;;IACA,MAAMQ,MAAM,GAAC,YAAU;MACrB,IAAG;QACK,MAAMC,GAAG,GAAC,MAAMV,KAAK,CAACW,IAAN,CAAW,eAAX,CAAhB;QACAC,OAAO,CAACC,GAAR,CAAY,gBAAgBH,GAAG,CAACI,IAAhC,EAFL,CAIK;;QACAN,MAAM,CAACO,IAAP,CAAY;UACAC,IAAI,EAAC;QADL,CAAZ;MAGP,CARD,CAQC,OAAMC,KAAN,EAAY;QACXL,OAAO,CAACC,GAAR,CAAYI,KAAZ;MACF;IACD,CAZD,CAHK,CAeJ;;;IACD,OAAO;MAACR;IAAD,CAAP;EAED,CAzBY,CAyBZ;;;AAzBY,CAAf","names":["axios","useRouter","props","parent_id","type","String","required","setup","router","logout","res","post","console","log","data","push","name","error"],"sourceRoot":"","sources":["C:\\vue_project\\vue3-first\\src\\ex18_component_store\\component\\navDo.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <nav class=\"navbar navbar-expand-sm right-block navbar-dark\" v-if=\"parent_id\">\r\n        <ul class=\"navbar-nav\">\r\n            <li class=\"nav-item nav-link\" @click=\"logout\">{{parent_id}}님(로그아웃)</li>\r\n              <li class=\"nav-item\">\r\n                <router-link class=\"nav-link\" to=\"{name='Member_Update'}\">정보수정</router-link>\r\n              </li>\r\n             