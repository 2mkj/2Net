{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport axios from '../../axios/axiossetting.js';\nimport { ref } from 'vue';\nexport default {\n  setup() {\n    const id = ref(\"\");\n    const name = ref(\"\");\n    const router = useRouter();\n\n    const ForgotPass = async () => {\n      try {\n        const res = await axios.post(\"forgot\", {\n          id: id.value,\n          name: name.value\n        });\n        const result = res.data;\n\n        if (result <= 0) {\n          alert(\"등록된 정보가 없습니다.\");\n        } else {\n          alert(\"회원님의 비밀번호는 \" + res.data.password + \" 입니다.\");\n          router.push({\n            name: \"Login\"\n          });\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    };\n\n    return {\n      id,\n      name,\n      ForgotPass\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA8BA,SAAQA,SAAR,QAAwB,YAAxB;AACA,OAAOC,KAAP,MAAkB,6BAAlB;AACA,SAAQC,GAAR,QAAkB,KAAlB;AACA,eAAe;EACXC,KAAK,GAAE;IACL,MAAMC,EAAC,GAAGF,GAAG,CAAC,EAAD,CAAb;IACA,MAAMG,IAAG,GAAGH,GAAG,CAAC,EAAD,CAAf;IACA,MAAMI,MAAK,GAAIN,SAAS,EAAxB;;IAEA,MAAMO,UAAS,GAAI,YAAY;MAC/B,IAAG;QACK,MAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,IAAN,CAAW,QAAX,EAAoB;UACpCL,EAAC,EAAIA,EAAE,CAACM,KAD4B;UAEpCL,IAAI,EAAEA,IAAI,CAACK;QAFyB,CAApB,CAAlB;QAII,MAAMC,MAAK,GAAIH,GAAG,CAACI,IAAnB;;QACA,IAAGD,MAAK,IAAK,CAAb,EAAe;UACbE,KAAK,CAAC,eAAD,CAAL;QACD,CAFD,MAEK;UACHA,KAAK,CAAC,gBAAcL,GAAG,CAACI,IAAJ,CAASE,QAAvB,GAAgC,OAAjC,CAAL;UACER,MAAM,CAACS,IAAP,CAAY;YACVV,IAAG,EAAI;UADG,CAAZ;QAGJ;MACT,CAdH,CAcG,OAAMW,GAAN,EAAU;QACTC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACF;IACD,CAlBD;;IAoBF,OAAO;MACLZ,EADK;MACDC,IADC;MACKE;IADL,CAAP;EAIA;;AA9BW,CAAf","names":["useRouter","axios","ref","setup","id","name","router","ForgotPass","res","post","value","result","data","alert","password","push","err","console","log"],"sourceRoot":"","sources":["C:\\Users\\user\\vue\\simple_vue_project\\Elv\\vue\\src\\component\\member\\member_forgot.vue"],"sourcesContent":["<template>\r\n  <form class='