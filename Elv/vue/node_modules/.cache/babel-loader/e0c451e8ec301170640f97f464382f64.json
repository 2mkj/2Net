{"ast":null,"code":"import { ref, watch } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    const store = useStore();\n    const maxpage = ref(0);\n    const currentpage = ref(1);\n    const pagelist = ref([]);\n    watch(() => store.state.obj, () => {\n      maxpage.value = store.state.obj.maxpage;\n      currentpage.value = store.state.obj.currentpage;\n      pagelist.value = store.state.obj.pagelist;\n    }); //페이지 네이션의 페이지 번호를 클릭한 경우\n    //store의 state.page 값을 선택한 페이지로 변경합니다.\n\n    const getList = page => {\n      store.dispatch('store_page', page);\n    };\n\n    return {\n      getList,\n      currentpage,\n      pagelist,\n      maxpage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwBA,SAASA,GAAT,EAAaC,KAAb,QAAyB,KAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,eAAe;EACXC,KAAK,GAAE;IACH,MAAMC,KAAI,GAAIF,QAAQ,EAAtB;IACA,MAAMG,OAAM,GAAIL,GAAG,CAAC,CAAD,CAAnB;IACA,MAAMM,WAAU,GAAIN,GAAG,CAAC,CAAD,CAAvB;IACA,MAAMO,QAAO,GAAIP,GAAG,CAAC,EAAD,CAApB;IAEAC,KAAK,CAAC,MAAIG,KAAK,CAACI,KAAN,CAAYC,GAAjB,EACL,MAAI;MACAJ,OAAO,CAACK,KAAR,GAAgBN,KAAK,CAACI,KAAN,CAAYC,GAAZ,CAAgBJ,OAAhC;MACAC,WAAW,CAACI,KAAZ,GAAoBN,KAAK,CAACI,KAAN,CAAYC,GAAZ,CAAgBH,WAApC;MACAC,QAAQ,CAACG,KAAT,GAAiBN,KAAK,CAACI,KAAN,CAAYC,GAAZ,CAAgBF,QAAjC;IACH,CALI,CAAL,CANG,CAaH;IACA;;IACA,MAAMI,OAAO,GAAEC,IAAD,IAAQ;MAClBR,KAAK,CAACS,QAAN,CAAe,YAAf,EAA6BD,IAA7B;IACJ,CAFA;;IAGA,OAAM;MACFD,OADE;MACOL,WADP;MACoBC,QADpB;MAC8BF;IAD9B,CAAN;EAGJ;;AAtBW,CAAf","names":["ref","watch","useStore","setup","store","maxpage","currentpage","pagelist","state","obj","value","getList","page","dispatch"],"sourceRoot":"","sources":["C:\\vue_project\\vue3-first\\src\\ex18_component_store\\component\\pageDo.vue"],"sourcesContent":["\r\n<template>\r\n    <ul class=\"pagination\">\r\n            <li v-if=\"currentpage !== 1\" class=\"page-item\">\r\n                <a style=\"cursor:pointer\" class=\"page-link\" @click=\"getList(currentpage - 1)\">\r\n                    Previous\r\n                </a>\r\n            </li>\r\n            <li v-for=\"page in pagelist\"\r\n                :key=\"page\"\r\n                class=\"page-item\"\r\n                :class=\"currentpage == page ? 'active' : '' \"\r\n            >\r\n            <a style=\"cursor:pointer\" class=\"page-link\" @click=\"getList(page)\">{{page}}</a>\r\n        </li>\r\n        <li v-if=\"maxpage > currentpage\" class=\"page-item\">\r\n            <a style=\"cursor:pointer\" class=\"page-link\" @click=\"getList(currentpage + 1)\">\r\n                Next\r\n            </a>\r\n        </li>\r\n    </ul>\r\n</template>\r\n\r\n<script>\r\nimport { ref,watch} from 'vue';\r\nimport { useStore } from 'vuex';\r\nexport default {\r\n    setup(){\r\n        const store = useStore();\r\n        const maxpage = ref(0);\r\n        const currentpage = ref(1);\r\n        const pagelist = ref([]);\r\n\r\n        watch(()=>store.state.obj,\r\n        ()=>{\r\n            maxpage.value = store.state.obj.maxpage;\r\n            currentpage.value = store.state.obj.currentpage;\r\n            pagelist.value = store.state.obj.pagelist;\r\n        });\r\n\r\n        //페이지 네이션의 페이지 번호를 클릭한 경우\r\n        //store의 state.page 값을 선택한 페이지로 변경합니다.\r\n        const getList=(page)=>{\r\n            store.dispatch('store_page', page);\r\n        }\r\n        return{\r\n            getList, currentpage, pagelist, maxpage\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}