tch(name, () => {\r\n            console.log(name.value);\r\n        })\r\n        watch(name2, () => {\r\n            console.log(name2.value);\r\n        })\r\n        return{\r\n            name, name2\r\n        };\r\n    }\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\nlabel {display:block}\r\n</style>"]},"metadata":{},"sourceType":"module"}